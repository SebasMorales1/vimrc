let mapleader=' '

set number
set relativenumber
syntax on
set termguicolors

set tabstop=4
set shiftwidth=4
set expandtab
set breakindent

set incsearch
set nocompatible
set showcmd
set encoding=utf-8

set background=dark
set mouse=a

filetype on
filetype plugin on
filetype plugin indent on
set showmatch
set laststatus=2
